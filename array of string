#include<stdio.h>
#include<string.h>
void main()
{
    char a[10][10];
    int b;
    printf("enter no.of names");
    scanf("%d",& b);
    printf("enter all names");
    for(int i=0;i<b;i++)
    {
        scanf(" %s",a[i]);
    }
    printf("the legends are");
    for(int i=0;i<b;i++)
    {
        printf("\n no %d . %s",i+1,a[i]);
    }
    }
