#include<stdio.h>
int main()
{
    int a;
    printf("enter any number");
    scanf("%d",&a);
    for(int i=5;i<=a;i++)
    {
        for(int j=4;j<i;j++)
        {
            for(int k=3;k<j;k++)
            {
                if(i*i==j*j+k*k)
                {
                    printf("\n%d %d %d",i,j,k);
                }
            }
        }
    }
}
